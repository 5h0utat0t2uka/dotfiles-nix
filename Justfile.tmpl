set dotenv-load := false

check:
  nix flake check ./nix-config

build:
  sudo darwin-rebuild switch --flake ./nix-config#A3112

only-update:
  nix flake update ./nix-config

full-update:
  nix flake update ./nix-config
  sudo darwin-rebuild switch --flake ./nix-config#A3112

gc:
  nix-collect-garbage -d
