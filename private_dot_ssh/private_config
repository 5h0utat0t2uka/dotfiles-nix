Include /Users/shouta/.config/colima/ssh_config

IgnoreUnknown UseKeychain

Host *
  UseKeychain yes
  AddKeysToAgent yes
  # IdentityFile ~/.ssh/id_ed25519

Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_ed25519_github
  AddKeysToAgent yes
  UseKeychain yes

Host vultr
  HostName 66.42.45.103
  Port 2222
  User shouta
  IdentityFile ~/.ssh/id_ed25519_vultr
  IdentitiesOnly yes

Host vultr-root
  HostName 66.42.45.103
  Port 2222
  User root
  IdentityFile ~/.ssh/id_ed25519_vultr-root
  IdentitiesOnly yes
